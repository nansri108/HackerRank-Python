if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    
    nancy=[]
    for i in range(x + 1): 
        for j in range(y + 1): 
            for m in range(z + 1): 
                if ((i + j + m) != n):
                    nancy.append([i,j,m])
                    
    print(nancy)
